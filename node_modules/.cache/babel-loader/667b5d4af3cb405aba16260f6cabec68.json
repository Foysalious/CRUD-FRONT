QAAA,KAAA,CAAA,CAAA,CAAA;AACA,OAPA;AAQA,KApBA;AAqBA,IAAA,aArBA,yBAqBA,IArBA,EAqBA;AAAA;;AACA,WAAA,MAAA,GAAA,IAAA;AAEA,MAAA,KAAA,CACA,IADA,CACA,KAAA,GADA,EACA;AACA,QAAA,KAAA,EAAA,IAAA,CAAA,KADA;AAEA,QAAA,WAAA,EAAA,IAAA,CAAA,WAFA;AAGA,QAAA,KAAA,EAAA,IAAA,CAAA;AAHA,OADA,EAMA,IANA,CAMA,YAAA;AACA,QAAA,MAAA,CAAA,WAAA;AACA,OARA,EASA,KATA,CASA,UAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,CAAA,CAAA;AACA,OAXA;AAYA,KApCA;AAqCA,IAAA,WArCA,uBAqCA,IArCA,EAqCA;AAAA;;AACA,WAAA,MAAA,GAAA,IAAA;AAEA,MAAA,KAAA,CACA,GADA,WACA,KAAA,GADA,cACA,IAAA,CAAA,EADA,GACA;AACA,QAAA,KAAA,EAAA,IAAA,CAAA,KADA;AAEA,QAAA,WAAA,EAAA,IAAA,CAAA,WAFA;AAGA,QAAA,KAAA,EAAA,IAAA,CAAA;AAHA,OADA,EAMA,IANA,CAMA,YAAA;AACA,QAAA,MAAA,CAAA,WAAA;AACA,OARA,EASA,KATA,CASA,UAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,CAAA,CAAA;AACA,OAXA;AAYA,KApDA;AAqDA,IAAA,QArDA,oBAqDA,EArDA,EAqDA;AACA;AAEA,WAAA,aAAA,CAAA,EAAA;AACA,KAzDA;AA0DA,IAAA,MA1DA,kBA0DA,IA1DA,EA0DA;AACA;AAEA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,MAAA,GAAA,IAAA;AACA,KA/DA;AAgEA,IAAA,YAhEA,wBAgEA,IAhEA,EAgEA;AACA;AAEA,UAAA,IAAA,CAAA,MAAA,EAAA;AACA;AACA,aAAA,WAAA,CAAA,IAAA;AACA,OAHA,MAGA;AACA;AACA,aAAA,aAAA,CAAA,IAAA;AACA;AACA;AA1EA,GAfA;AA2FA,EAAA,OA3FA,qBA2FA;AACA,SAAA,WAAA;AACA;AA7FA,CAAA","sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <div class=\"ui fixed inverted menu vue-color\">\r\n      <div class=\"ui container\">\r\n        <a href=\"#\" class=\"header item\"> VUE CRUD with Laravel API weDevs </a>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"ui main container\">\r\n      <MyForm :form=\"form\" @onFormSubmit=\"onFormSubmit\" />\r\n      <Loader v-if=\"loader\" />\r\n      <ProductList\r\n        :products=\"products\"\r\n        @onDelete=\"onDelete\"\r\n        @onEdit=\"onEdit\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport MyForm from \"./components/MyForm\";\r\nimport ProductList from \"./components/ProductList\";\r\nimport Loader from \"./components/Loader\";\r\n\r\nexport default {\r\n  name: \"App\",\r\n  components: {\r\n    MyForm,\r\n    ProductList,\r\n    Loader\r\n  },\r\n  data() {\r\n    return {\r\n      url: \"http://localhost:8000/api/Product\",\r\n      products: [],\r\n      form: { title: \"\", description: \"\", price: \"\", isEdit: false },\r\n      loader: false\r\n    };\r\n  },\r\n  methods: {\r\n    getProducts() {\r\n      this.loader = true;\r\n\r\n      axios.get(this.url).then(data => {\r\n        this.products = data.data;\r\n        this.loader = false;\r\n      });\r\n    },\r\n    deleteProduct(id) {\r\n      this.loader = true;\r\n\r\n      axios\r\n        .delete(`${this.url}/${id}`)\r\n        .then(() => {\r\n          this.getProducts();\r\n        })\r\n        .catch(e => {\r\n          alert(e);\r\n        });\r\n    },\r\n    createProduct(data) {\r\n      this.loader = true;\r\n\r\n      axios\r\n        .post(this.url, {\r\n          title: data.title,\r\n          description: data.description,\r\n          price: data.price\r\n        })\r\n        .then(() => {\r\n          this.getProducts();\r\n        })\r\n        .catch(e => {\r\n          alert(e);\r\n        });\r\n    },\r\n    editProduct(data) {\r\n      this.loader = true;\r\n\r\n      axios\r\n        .put(`${this.url}/${data.id}`, {\r\n          title: data.title,\r\n          description: data.description,\r\n          price: data.price\r\n        })\r\n        .then(() => {\r\n          this.getProducts();\r\n        })\r\n        .catch(e => {\r\n          alert(e);\r\n        });\r\n    },\r\n    onDelete(id) {\r\n      // window.console.log(\"app delete \" + id);\r\n\r\n      this.deleteProduct(id);\r\n    },\r\n    onEdit(data) {\r\n      // window.console.log(\"app edit \", data);\r\n\r\n      this.form = data;\r\n      this.form.isEdit = true;\r\n    },\r\n    onFormSubmit(data) {\r\n      // window.console.log(\"app onFormSubmit\", data);\r\n\r\n      if (data.isEdit) {\r\n        // call edit product\r\n        this.editProduct(data);\r\n      } else {\r\n        // call create product\r\n        this.createProduct(data);\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.getProducts();\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.vue-color {\r\n  background: #41b883 !important;\r\n}\r\n\r\n.main.container {\r\n  margin-top: 60px;\r\n}\r\n\r\n.submit-button {\r\n  margin-top: 24px !important;\r\n  float: right;\r\n}\r\n\r\n.data {\r\n  margin-top: 15px;\r\n}\r\n\r\nthead tr th {\r\n  background: #e0e0e0 !important;\r\n}\r\n\r\n.ui.inverted.dimmer {\r\n  background-color: rgba(255, 255, 255, 0) !important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src"}]}